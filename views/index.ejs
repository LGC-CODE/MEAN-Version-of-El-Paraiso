		<ui-view></ui-view>
	<script type="text/ng-template" id="/home.html"> <!--home ctrl-->
		<header ng-init="homeBackground()">
			<section class="hero">
				<div class="container">
					<div class="row hero-content">
						<div class="col-md-12 col-lg-12 visible-md visible-lg">
							<img class="img-adjust animated fadeInLeft" width="43%" src="img/logo.png">
						</div>
						<div class="col-md-12 text-center" ng-click="soundStart()">
							<h1 class="animated fadeInDown">El Paraiso</h1>
							<h2>Nieves Y Raspados</h2>
							<a href="#/gallery" class="use-btn animated fadeInUp">View Gallery</a> <a href="#/info" class="learn-btn animated fadeInUp">View Address<i class="fa fa-arrow-down"></i></a>
						</div>
						<div class="col-md-6 col-md-offset-3 col-xs-offset-0">
							<div class="animated fadeInLeft comment-wrap">
								<h2>Comments:</h2>
								<div class="comment" ng-repeat="comment in comments | limitTo: quantity | orderBy: random">
									<h3>Comment:</h3 <br> <h2>{{ comment.comment }}</h2>
									<h3>City: {{ comment.city }}</h3>
									<h3>By: {{ comment.name }}</h3>
								</div>
								<br>
								<button ng-click="loadMore()" class="btn btn-default">View More</button><br>
							</div>
							<br>
							<div ng-show="alert" class="alert alert-warning row">
							    <span>{{ alert.message }}</span>
						    </div>
						    <br>
							<form ng-submit="writeComment()" class="form-group">
								<input type="text" ng-model="name" placeholder="Name" class="form-control"><br>
								<input type="text" ng-model="city" placeholder="City" class="form-control"><br>
								<input type="text" ng-model="comment" placeholder="Leave a Comment!" class="form-control"><br>
								<button type="submit" ng-click="hideAlert()" class="btn btn-default">Post!</button>
							</form>
						</div>
					</div>
				</div>
			</section>
		</header>
	</script>
	<script type="text/ng-template" id="/info.html">
		<div class="info animated zoomInDown">
			<div class="container">
				<div class="col-md-12 address">
					<div class="col-md-6">
						<h2>We Value and Appreciate Every Customer</h2>
						<p>Thank You for Choosing Us!</p>
					</div>
					<div class="col-md-6">
						<a href="#/about"><button class="btn btn-default">Continue</button></a>
					</div>
				</div>
				<div class="col-md-6">
					<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3154.2837503645046!2d-121.43717234961763!3d37.75994437966286!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x80903d576f6d943b%3A0xeb26a334d40fb1b5!2s3244+N+Tracy+Blvd%2C+Tracy%2C+CA+95376!5e0!3m2!1sen!2sus!4v1469149401663" width="100%" height="300" frameborder="0" style="border:0" allowfullscreen></iframe>
				</div>
				<div class="col-md-6">
					<address>
						<h1>3244 N Tracy Blvd</h1>
						<h1>Tracy, CA 95376</h1>
					</address>
					<div class="success">Email Generated Successfully</div>
					<div class="email">
						<form ng-submit="emailUs()">
							<div class="form-group">
								<label>Subject:</label>
								<input ng-model="subject" class="form-control" type="text/plain">
							</div>
							<div class="form-group">
								<label>Message:</label>
								<input ng-model="body" class="form-control" type="text/plain">
							</div><br>
							<button type="submit" class="btn btn-default">Email Us</button> 
						</form>
					</div>
				</div>
			</div>
		</div>
	</script>
	<script type="text/ng-template" id="/loginUp.html">

		<div class="login">

			<div ng-show="error" class="alert alert-danger row">
			    <span>{{ error.message }}</span>
		    </div>

			<form ng-hide="isLoggedIn()" ng-submit="getUser()" class="form-group container">
				<div class="col-md-4">
					<input class="form-control" type="text" ng-model="username" placeholder="Usuario"><br/>
					<input class="form-control" type="password" ng-model="password" placeholder="Clave"><br/>
					<button type="submit">Acceder</button>
				</div>
			</form>

		</div>
	</script>
	<script type="text/ng-template" id="/private.html">
		
			<div ng-init="privateBackground()" ng-show="isLoggedIn()" class="private">
			<div ng-show="alert" class="success-btn alert alert-warning">{{ alert.message }}</div>
			<div class="container col-md-offset-4">
					<form ng-submit="getUrl()" class="form-group col-md-4">						
							<input class="form-control" type="text" ng-model="url" placeholder="Direccion URL"><br><br>
							<input class="form-control" type="text" ng-model="des" placeholder="Descripcion">
							<br/>
							<button class="btn btn-default" ng-click="hideAlert()" type="submit">Subir Foto</button><br>
							<button ng-click="logOut()" class="logout btn btn-default">Salir</button>
					</form>
			</div>

	</script>
