								<div class="caption-content">
									<a ng-href="{{ pics.url }}" class="single_image">
										<i class="fa fa-search"></i><br>
										<p>{{ pics.description }}</p>
									</a>
								</div>
								</figcaption>
							</figure>
						</li>						
					</ul>
				</div>
			</div>
		</section>
